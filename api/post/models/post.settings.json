{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "published_at": {
      "type": "datetime"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "category": {
      "via": "posts",
      "model": "category"
    }
  }
}
